programa {
  funcao inicio() {

    real valorPro, qntComp, dinheiroRec, troco

    escreva(" Preço do produto: ")
    leia(valorPro)

    escreva("\n Quantidade comprada: ")
    leia(qntComp)

    escreva("\n Dinheiro Recebido: ")
    leia(dinheiroRec)

    troco = dinheiroRec - (valorPro * qntComp)

    escreva("\n TROCO: ",troco)






    
  }
}
