programa {
  funcao inicio() {

    inteiro valorX, valorY, resultado1, resultado2

    escreva(" Digite o valor de X: ")
    leia(valorX)
    escreva("\n Digite o valor de Y: ")
    leia(valorY)

    resultado1 = valorX + valorY
    resultado2 = valorX - valorY

    escreva("\n Resultado da soma: ",resultado1)
    escreva("\n Resultado da subtração: ",resultado2)

    
  }
}
